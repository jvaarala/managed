source "./adapters/symlink"

install_zshrc() {
  echo ""
  echo "ğŸ”§ Setting up .zshrc configuration..."
  install_symlink "$HOME/managed/.zshrc" "$HOME/.zshrc"
}

uninstall_zshrc() {
  echo ""
  echo "ğŸ—‘ï¸ Removing .zshrc configuration..."
