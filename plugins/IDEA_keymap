install_IDEA_keymap() {
    echo ""
    echo "ðŸ”§ Setting up IntelliJ IDEA keymap..."
    install_symlink "IdeaKeymapMacOS.xml" "$HOME/Library/Application Support/JetBrains/IntelliJIdea2024.3/keymaps/IdeaKeymapMacOS.xml"
}

uninstall_IDEA_keymap() {
    echo ""
    echo "ðŸ”§ Removing IntelliJ IDEA keymap..."
    uninstall_symlink "IdeaKeymapMacOS.xml" "$HOME/Library/Application Support/JetBrains/IntelliJIdea2024.3/keymaps/IdeaKeymapMacOS.xml"
}
